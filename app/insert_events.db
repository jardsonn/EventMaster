  -- Inserir salas relacionadas aos eventos
  INSERT INTO salas (eventoId, nome, capacidadeMaxima)
  SELECT
    eventos.id,
    CONCAT('Sala ', salas.numero),
    FLOOR(RAND() * 50) + 1 -- Capacidade máxima entre 1 e 50
  FROM
    eventos
  JOIN
    (SELECT 1 AS numero UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5) AS salas
  WHERE
    eventos.id BETWEEN 1 AND 20;

--    Transformar data em long
    UPDATE eventosÉ
    SET data = CAST(strftime('%s', data) AS INTEGER) * 1000
    WHERE id NOT IN (1, 2, 3);